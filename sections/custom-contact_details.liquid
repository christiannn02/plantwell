                      }
                  ],
                  "default": "4"
              },
              {
                "type": "select",
                "id": "md_banner_col",
                "label": "Tablet",
                "options":[
                      {
                          "label": "Gird 6",
                          "value": "6"
                      },
                      {
                          "label": "Gird 4",
                          "value": "4"
                      },
                      {
                          "label": "Gird 3",
                          "value": "3"
                      }
                  ],
                  "default": "4"
              }
          ]
       },
	   {
          "type": "special_note",
          "name": "Special Note",
		   "settings": [
              {
                "type": "text",
                "id": "special_icon",
                "label": "Special Icon",
                "default": "ios-clock-outline",
                "info": "Ionicons Icon [See Icon](https:\/\/ionicons.com)"
              },
              {
                  "type": "text",
                  "id": "special_title",
                  "label": "Special Title",
                  "default": "HOUR OF OPERATION"
              },
              {
                  "type": "textarea",
                  "id": "special_content",
                  "label": "Special Content",
                  "default": "Monday – Friday : 09:00 – 20:00 <br> Sunday & Saturday: 10:30 – 22:00"
              },
			  {
                "type": "select",
                "id": "lg_banner_col",
                "label": "Desktop",
                "options":[
                      {
                          "label": "Gird 6",
                          "value": "6"
                      },
                      {
                          "label": "Gird 4",
                          "value": "4"
                      },
                      {
                          "label": "Gird 3",
                          "value": "3"
                      }
                  ],
                  "default": "4"
              },
              {
                "type": "select",
                "id": "md_banner_col",
                "label": "Tablet",
                "options":[
