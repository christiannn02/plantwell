            {% if section.settings.logo_2 %}
            <div class="mb--30 mb-md--20 max-w-100">
              <img src="{{ section.settings.logo_2 | img_url: '300x300' }}" alt="{{ section.settings.logo_2.alt | default: shop.name }}">
            </div>
            {% endif %}
