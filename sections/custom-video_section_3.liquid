            <h1 class="heading-secondary mb--40 mb-md--20">{{ section.settings.about_title }}</h1>
              {%- endif -%}
            <p class="font-2 heading-color font-size-16 mb--30 mb-md--20">{{ section.settings.about_description }}</p>
