        .productmsg {
            font-size: 12px;
        }
    }
    .popup-content {
        flex: 0 0 45%;
        background: aliceblue;
        padding: 20px 0 0 0;

        p.total_item {
            font-size: 10px;
        }
        p.cart_total {
            font-weight: bold;
        
        .total_price_label {
            color: #000;
        }

        .shopping-cart__total {
            color: #00A651;
            }
        }
    }
    .modal-button {
        flex: 0 0 36%;
       a#c {
        margin: 0;
