.newsletter-content {
    padding: 10px 10px 10px 10px;
    text-align: center;
    height: 20rem;
    max-width: 30rem;

    .warning-logo {
        text-align: center;
        font-size: 40px;
        margin-top: 30px;
        margin-bottom: 30px;
    }

    .warning-button {
        margin-top: 10px;
    }

        p {
            margin-top: -50px;
        }
    #proceed {
        background: #00a651;
        color: #fff;
        padding: 8px 15px 8px 15px;
        border: 2px solid #00a651;
        border-radius: 3px;
        font-size: 12px;
        margin-left: 7px;

        &:hover {
            background: #fff;
            color: #00a651;
            border: 2px solid #00a651;
        }
    }
    #leave {
        color: red;
        padding: 8px 15px 8px 15px;
        border: 2px solid red;
        border-radius: 3px;
        font-size: 12px;

        &:hover {
            background: red;
            color: #fff;
            border: 2px solid red;
        }
    }
}

